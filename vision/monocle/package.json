{
  "version": "0.0.1",
  "description": "UI for Cortex",
  "scripts": {
    "app": "npm run build & npm run cortex",
    "build": "rollup -c",
    "dev": "npm run watch & npm run cortex",
    "watch": "chokidar 'src/**' -c 'npm run build'",
    "cortex": "browser-sync start --files 'dist/*, index.html' --server --port 13100"
  },
  "devDependencies": {
    "browser-sync": "^2.24.6",
    "chokidar-cli": "^1.2.0",
    "dotenv": "^6.0.0",
    "path": "^0.12.7",
    "rollup": "^0.63.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "@cycle/dom": "^20.4.0",
    "@cycle/isolate": "^3.4.0",
    "@cycle/run": "^4.4.0",
    "xstream": "^11.7.0"
  },
  "license": "GPL-3.0-or-later",
  "author": "jason a. grafft <jason@grafft.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jagrafft/vision"
  }
}
